{
    "name": "trinityrank/geo-location",
    "description": "GeoLocation - show or hide operaters in selected countries",
    "keywords": [
        "laravel",
        "nova",
        "trinityrank",
        "geo-location"
    ],
    "homepage": "https://github.com/trinity-rank/geo-location",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "TrinityRank DevTeam",
            "email": "trinitydevbg@gmail.com",
            "role": "Team"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Trinityrank\\GeoLocation\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinityrank\\GeoLocation\\GeoLocationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
